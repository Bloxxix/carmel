{
  "name": "carmel",
  "productName": "carmel",
  "version": "1.0.0",
  "description": "Carmel Studio",
  "main": ".webpack/main",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "build": "theia build --mode development",
    "publish": "electron-forge publish",
    "lint": "eslint --ext .ts ."
  },
  "keywords": [],
  "author": {
    "name": "I. Dan Calinescu",
    "email": "idancalinescu@gmail.com"
  },
  "license": "MIT",
  "theia": {
    "target": "electron"
  },
  "config": {
    "forge": {
      "packagerConfig": {
        "icon": "./assets/icon.icns",
        "protocols": [
          {
            "name": "carmel",
            "schemes": [
              "carmel"
            ]
          }
        ],
        "extraResource": [
          "./assets/"
        ]
      },
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "carmel"
          }
        },
        {
          "name": "@electron-forge/maker-dmg",
          "icon": "./assets/icon.icns",
          "background": "./assets/TestBkg.png",
          "format": "ULFO",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ],
      "plugins": [
        [
          "@electron-forge/plugin-webpack",
          {
            "mainConfig": "./webpack.main.config.js",
            "renderer": {
              "config": "./webpack.renderer.config.js",
              "entryPoints": [
                {
                  "html": "./src/app/index.html",
                  "js": "./src/app/index.tsx",
                  "name": "main_window"
                }
              ]
            }
          }
        ]
      ]
    }
  },
  "devDependencies": {
    "@electron-forge/cli": "6.0.0-beta.51",
    "@electron-forge/maker-deb": "6.0.0-beta.51",
    "@electron-forge/maker-dmg": "6.0.0-beta.51",
    "@electron-forge/maker-rpm": "6.0.0-beta.51",
    "@electron-forge/maker-squirrel": "6.0.0-beta.51",
    "@electron-forge/maker-zip": "^6.0.0-beta.51",
    "@electron-forge/plugin-webpack": "6.0.0-beta.51",
    "@marshallofsound/webpack-asset-relocator-loader": "^0.5.0",
    "@typescript-eslint/eslint-plugin": "^2.18.0",
    "@typescript-eslint/parser": "^2.18.0",
    "css-loader": "^3.6.0",
    "electron": "9.0.4",
    "electron-rebuild": "^1.11.0",
    "eslint": "^6.8.0",
    "eslint-plugin-import": "^2.20.0",
    "fork-ts-checker-webpack-plugin": "^3.1.1",
    "node-loader": "^0.6.0",
    "style-loader": "^0.23.1",
    "ts-loader": "^6.2.1",
    "typescript": "^3.7.0"
  },
  "dependencies": {
    "@ant-design/dark-theme": "^2.0.2",
    "@carmel/cli": "^1.9.13",
    "@react-native-community/async-storage": "^1.11.0",
    "@types/command-exists": "^1.2.0",
    "@types/deep-extend": "^0.4.31",
    "@types/react-dom": "^16.9.8",
    "@types/react-redux": "^7.1.9",
    "@types/react-router-dom": "^5.1.5",
    "@types/react-slick": "^0.23.4",
    "@types/recursive-readdir": "^2.2.0",
    "@types/shortid": "0.0.29",
    "ace-builds": "^1.4.12",
    "antd": "^4.4.1",
    "codemirror": "^5.55.0",
    "command-exists": "^1.2.9",
    "connected-react-router": "^6.8.0",
    "decompress": "^4.2.1",
    "decompress-targz": "^4.1.1",
    "deep-extend": "^0.6.0",
    "electron-dl": "^3.0.1",
    "electron-squirrel-startup": "^1.0.0",
    "file-loader": "^6.0.0",
    "less-loader": "^6.2.0",
    "monaco-editor-webpack-plugin": "^1.9.0",
    "react": "^16.13.1",
    "react-ace": "^9.1.1",
    "react-awesome-reveal": "^2.4.2",
    "react-codemirror2": "^7.2.1",
    "react-dom": "^16.13.1",
    "react-hot-loader": "^4.12.21",
    "react-monaco-editor": "^0.39.1",
    "react-redux": "^7.2.0",
    "react-router-dom": "^5.2.0",
    "react-slick": "^0.27.0",
    "react-treebeard": "^3.2.4",
    "recursive-readdir": "^2.2.2",
    "redux": "^4.0.5",
    "redux-persist": "^6.0.0",
    "shortid": "^2.2.15",
    "ts-import-plugin": "^1.6.6",
    "url-loader": "^4.1.0"
  }
}
